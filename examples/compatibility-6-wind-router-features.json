{
  "compatibilities": [
    {
      "features": [
        {
          "cpu.model": {
            "vendor_id": "Intel"
          }
        }
      ],
      "description": "required cpu vendor"
    },
    {
      "features": [
        {
          "kernel.config": {
            "CGROUP_BPF": "y",
            "XFRM_INTERFACE": "y",
            "NF_TABLES": "y",
            "NETFILTER_XT_TARGET_NOTRACK": "y",
            "NET_ACT_BPF": "y",
            "MPLS_ROUTING": "y",
            "MPLS_IPTUNNEL": "y",
            "PPPOE": "y"
          },
          "kernel.loadedmodule": [
            "br_netfilter",
            "ebtables",
            "ifb",
            "ip6_tables",
            "ip_tables",
            "mpls_iptunnel",
            "mpls_router",
            "nf_conntrack",
            "overlay",
            "ppp_generic",
            "vfio-pci",
            "vhost-net",
            "vrf"
          ]
        }
      ],
      "description": "required kernel configuration"
    },
    {
      "features": [
        {
          "kernel.version": {
            "full": ">=5.0.0-.*-gcp,<6.10.0-.*-gcp"
          },
          "system.os_release": {
            "ID": "ubuntu",
            "VERSION_ID": "20.04;22.04"
          }
        }
      ],
      "description": "required configuration for GCP ubuntu version",
      "tags": ["ubuntu-gcp-os"]
    },
    {
      "features": [
        {
          "system.os_release": {
            "ID": "rhel",
            "VERSION_ID": "8"
          }
        }
      ],
      "description": "required configuration for rhel",
      "tags": ["rhel-os"]
    },
    {
      "features": [
        {
          "network.sriov": [
            {
              "capable": true,
              "configured": true
            }
          ]
        }
      ],
      "description": "required network configuration"
    }
  ]
}